/* pages/userList/editFood.wxss */
.container{
    margin-top:60rpx;
}
.handle-box{
    width:100%;
    padding:22rpx 10rpx;
}
.handle-box view{
    width:50%;
    display:inline-block;
    text-align:left;
}
.handle-box view:last-child{
    text-align:right;
}
.loadMore{
  width:100%;
  text-align: center;
}
.login{
    width:100%;
    position: absolute;
    left:0;
    top:40%;
    /* transform: (0,-40%); */
    text-align: center;
}
.clickLogin01{
    width:100%;
    font-size:16pt;
    margin-bottom: 30rpx;
}
.clickLogin02{
    width:100%;
    font-size:13pt;
}
page{
    background:#F9F5EC;
}

.edit-box{
    width:100%;
    height:100%;
    position:absolute;
    left:0;
    top:0;
    background:rgba(255,255,255,0.5);

}
.edit-box .delete button{
    width:200rpx;
    background:#FFCC66;
    color:#fff;
}
button::after{
    border:none;
}
.edit-content{
     background:#fff;
    width:420rpx;
    height:100%;
    position:absolute;
    right:0;
    top:0;
}

.edit-item{
    width:100%;
    height:100%;
}

.delete {
    position:absolute;
    top:50%;
    left:50%;
    transform:translate(-50%,-50%);
}
.fixed-title{
    width:100%;
    height:80rpx;
    position: fixed;
    top:0;
    left:0;
    background:#F3EBDF;
    
}
.fixed-title view{
    /* width:33.333%; */
    height:100%;
    line-height:80rpx;
    background:#F3EBDF;
    display:inline-block;
    text-align: center;
}
.fixed-title view:first-child{
    width:33.3%;
}
.fixed-title view:nth-child(2){
    width:33.4%;
}
.fixed-title view:last-child{
    width:33.3%;
}
.fixed-title view.chosed{
    background: #F9F5EC;
}

/**审核未通过列表**/
.like-list .failed-item{
    width:80rpx;   
     height:40rpx; 
     position: absolute;
     right:0;
     top:0;
    background:rgba(190,190,190,1);
    text-align: center;
    line-height: 40rpx;
    font-size: 11pt;
}
.tips{
    font-size: 11pt;
    margin-top:10rpx;
    margin-bottom:10rpx;
    color:rgba(168,168,168,1);
}